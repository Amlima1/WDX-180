{
	"WDX:Kramdown target _blank": {
    "prefix": "_blank",
    "body": [
      "{:target=\"_blank\"}"
    ],
    "description": "WDX:Kramdown target _blank"
  },
  "WDX SGEN EXERCISE 1": {
    "prefix": "wdx-sgen-ex-1",
    "body": [
      "<!-- WDX:SGEN:PROGRESS:task=Complete the exercise '${1:title}'|user_folder=${2:folder} -->",
      "<!-- WDX:SGEN:TESTS:name=Test Exercise: '${1:title}'|type=exist|user_folder=${2:folder}|files=${3:files} -->"
    ],
    "description": "Adds 2 comments with WDX:SGEN:PROGRESS and WDX:SGEN:TESTS (for type='exist' tests) for WDX exercises."
  },
  "WDX SGEN PROGRESS": {
    "prefix": "wdx-sgen-prog",
    "body": [
      "<!-- WDX:SGEN:PROGRESS:task=${1:title} -->"
    ],
    "description": "Adds 1 comment with WDX:SGEN:PROGRESS with Read '<task>'"
  }
}